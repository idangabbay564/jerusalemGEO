    -moz-box-shadow: 0px 0px 2px 3px rgba(0, 0, 0, 0.1);
    box-shadow: 0px 0px 2px 3px rgba(0, 0, 0, 0.1);
    position: absolute;
    top: 10px;
    left: 10px;
    z-index:9;
}

.counted{
  font-size: 14px
}


